<h1>New E-Scooter </h1>

<div class="container">
  <%= simple_form_for @e_scooter do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :street_name %>
    <%= f.input :disponibility_start %>
    <%= f.input :disponibility_end %>
    <%= f.submit "Create new e-scooter", class: "btn btn-secondary" %>
<% end %>
<%= link_to 'Homepage', root_path, class: "btn btn-secondary mt-3 mb-3" %>
</div>
